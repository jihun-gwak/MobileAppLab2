{
  "expo": {
    "name": "MobileAppLab2",
    "slug": "MobileAppLab2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
